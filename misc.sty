%
% Theorem, Lemma, etc environments (needs amsthm package)
%
% The xxxaux (where, e.g., xxx=lemma) environments are the original amsthm ones
% The xxx environments are then just wrappers that add a \noindent after
%     the environment. So the paragraph after the environment will not be
%     indented, even if paragraph intendation is on.
%
\theoremstyle{plain}
\newtheorem{theoremaux}{Theorem}%[chapter]
\newenvironment{theorem}{\begin{theoremaux}}{\end{theoremaux}\parfillskip=0pt\finalhyphendemerits=0\goodbreak\noindent\ignorespacesafterend}
%
\newtheorem{lemmaaux}[theoremaux]{Lemma}
\newenvironment{lemma}{\begin{lemmaaux}}{\end{lemmaaux}\parfillskip=0pt\finalhyphendemerits=0\goodbreak\noindent\ignorespacesafterend}
%
\newtheorem{corollaryaux}[theoremaux]{Corollary}
\newenvironment{corollary}{\begin{corollaryaux}}{\end{corollaryaux}\parfillskip=0pt\finalhyphendemerits=0\goodbreak\noindent\ignorespacesafterend}
%
\newtheorem{propositionaux}[theoremaux]{Proposition}
\newenvironment{proposition}{\begin{propositionaux}}{\end{propositionaux}\parfillskip=0pt\finalhyphendemerits=0\goodbreak\noindent\ignorespacesafterend}
%
\newtheorem{exampleaux}[theoremaux]{Example}
\newenvironment{example}{\begin{exampleaux}}{\end{exampleaux}\parfillskip=0pt\finalhyphendemerits=0\goodbreak\noindent\ignorespacesafterend}
%
\newtheorem{observationaux}[theoremaux]{Observation}
\newenvironment{observation}{\begin{observationaux}}{\end{observationaux}\parfillskip=0pt\finalhyphendemerits=0\goodbreak\noindent\ignorespacesafterend}
%
\newtheorem{claimaux}[theoremaux]{Claim}
\newenvironment{claim}{\begin{claimaux}}{\end{claimaux}\parfillskip=0pt\finalhyphendemerits=0\goodbreak\noindent\ignorespacesafterend}
%
\newtheorem{factaux}[theoremaux]{Fact}
\newenvironment{fact}{\begin{factaux}}{\end{factaux}\parfillskip=0pt\finalhyphendemerits=0\goodbreak\noindent\ignorespacesafterend}
%
%\theoremstyle{definition}
%\newtheorem{definitionaux}[theoremaux]{Definition}
%\newenvironment{definition}{\begin{definitionaux}}{\end{definitionaux}\parfillskip=0pt\finalhyphendemerits=0\goodbreak\noindent\ignorespacesafterend}
%
\theoremstyle{definition}
\newtheorem{definitionaux}[theoremaux]{Definition}
\newenvironment{definition}{\begin{definitionaux}}{\unskip\nobreak\hfil\penalty50
   \hskip2em\hbox{}\nobreak\hfil
   $\triangle$
   \parfillskip=0pt \finalhyphendemerits=0
   \end{definitionaux}\noindent\ignorespacesafterend}

